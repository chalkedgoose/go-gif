{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Cross-Origin-Embedder-Policy",
        "value": "*"
      } ]
    }, {
      "source": "**",
      "headers": [ {
        "key": "Cross-Origin-Opener-Policy",
        "value": "*"
      } ]
    } ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
